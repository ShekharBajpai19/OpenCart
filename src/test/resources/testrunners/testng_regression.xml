<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4" parallel = "tests" verbose="4">
  
  <listeners>
    <listener class-name="qa.opencart.listeners.ExtentReportListener" />
    <listener class-name="qa.opencart.listeners.TestAllureListener" />
   </listeners>
  
  <test name="Login Page Test">
    <classes>
       <class name="qa.opencart.tests.LoginPageTest"/>       
    </classes>
  </test>
  
  <test name="Accounts Page Test">
    <classes>
      <class name="qa.opencart.tests.AccountsPageTest"/>
      
    </classes>
    
    
  </test>
</suite>

